.network_image{
  position: relative;
  height:400rpx;
}
.to_images{
  position:absolute;
  right:20rpx;
  bottom:22rpx;
  border-radius:100%;
  padding-top:48rpx;
  width:90rpx;
  height:90rpx;
  font-size:18rpx;
  color:#666666;
  text-align: center;
  box-sizing: border-box;
  background:rgba(0,0,0,.3) url(data:image/png;base64,) no-repeat center 19rpx;
  background-size: 28rpx 26rpx;
}
.to_images text{
  color:#fff;
}
.network_addr{
  padding:38rpx 20rpx 38rpx 70rpx;
  font-size:24rpx;
  color:#333333;
  background:#fff url(data:image/png;base64,) no-repeat 20rpx center;
  background-size: 30rpx 42rpx;
  position: relative;
}
.network_addr::after{
  content:'';
  position: absolute;
  top:50%;
  right:20rpx;
  transform: translateY(-50%) rotate(45deg);
  border-top:1px solid #d0d0d0;
  border-right:1px solid #d0d0d0;
  width:12rpx;
  height:12rpx;
}
.distance{
  position:absolute;
  right:57rpx;
  top:50%;
  transform: translateY(-50%);
  font-size:22rpx;
  color:#999999;
}

.network_introduce{
  margin:20rpx 0;
  padding:36rpx 20rpx 50rpx 20rpx;
  background:#fff;
  font-size:28rpx;
}
.network_introduce .title{
  margin-bottom:28rpx;
}
.network_introduce .content{
  font-size:24rpx;
  color:#666666;
  line-height:41rpx;
}

.network_coupon{
  padding:0 20rpx 34rpx;
  font-size:28rpx;
  color:#333333;
  background:#fff;
}
.network_coupon .title{
  border-bottom:1px solid #eeeeee;
  padding:25rpx 20rpx;
  margin:0 -20rpx 30rpx;
}

.coupons{
  display: flex;
  flex-wrap: wrap;
}
.coupon{
  margin-bottom:20rpx;
  padding:17rpx 145rpx 15rpx 13rpx;
  width:345rpx;
  height:117rpx;
  background:#31a8e5;
  color:#fff;
  position: relative;
  box-sizing: border-box;
}
.coupon.is_get{
  background:#cecece;
}
.coupon::after,
.coupon::before{
  content:"";
  position: absolute;
  left:0;
  top:0;
  width:100%;
  height:100%;
  background-image: radial-gradient(circle,#ffffff,#ffffff 4rpx,transparent,transparent 10rpx);
  background-size:22rpx 22rpx;
  background-repeat: repeat-y;
  background-position: -3.7% top;
}
.coupon::before{
  background-position: 103.7% top;
}
.coupon:nth-child(odd){
  margin-right:auto;
}
.coupon_status{
  position:absolute;
  right:0;
  top:9rpx;
  bottom:9rpx;
  border-left:1px dashed #fff;
  padding:16rpx 10rpx 0;
  font-size:18rpx;
  text-align: center;
  z-index: 900;
}
.coupon_status .coupon_time{ 
  margin-top:25rpx;
  font-size:14rpx;
  color:#99ccff;
}
.coupon.is_get .coupon_time{
  color:#ffffff;
}
.coupon_name{
  font-size:20rpx;
  font-weight: 800;
  position: relative;
}
.coupon_name>view:last-child{
  position: absolute;
  right:0;
  top:0;
}
.coupon_name>view:last-child text{
  font-size:42rpx;
  font-family: Impact;
}
.coupon_addr{
  position:absolute;
  bottom:15rpx;
  left:13rpx;
  right:145rpx;
  margin-top:30rpx;
  font-size:14rpx;
}