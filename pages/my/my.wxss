.userInfo{
  padding:90rpx 0 95rpx 207rpx;
  margin-bottom:40rpx;
  background:#fff;
  position: relative;
  font-size: 36rpx;
}
.userHead{
  position:absolute;
  top:35rpx;
  left:34rpx;
  padding:5rpx;
  border: 1px solid #e6e6e6;
  border-radius: 100%;
  box-sizing: border-box;
  width:140rpx;
  height:140rpx;
  overflow: hidden;
}
.userHead image{
  border-radius: 120rpx;
  width: 100%;
  height:100%;
}

.items{
  padding-left: 34rpx;
  background:#fff;
  font-size:26rpx;
}
.item{
  position: relative;
  border-bottom:1px solid #e5e5e5;
  padding:37rpx 0 36rpx 69rpx;
  background:url(data:image/png;base64,) no-repeat left center;
  background-size: 40rpx auto;

}
.item.coupon{
  background-image: url(data:image/png;base64,)
}
.item.phone{
  background-image: url(data:image/png;base64,)
}
.item.phone text{
  color:#999999;
}
.item::before{
  content: "";
  position: absolute;
  top:50%;
  right:40rpx;
  width:8px;
  height:8px;
  border:1px solid transparent;
  border-top-color: #d0d0d0;
  border-right-color: #d0d0d0;
  transform: rotate(45deg) translateY(-50%);
}